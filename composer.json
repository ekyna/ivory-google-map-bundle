{
    "name": "ivory/google-map-bundle",
    "type": "symfony-bundle",
    "description": "Provides a google map integration for Symfony",
    "keywords": [ "google", "map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "symfony/framework-bundle": "^3.0 || ^4.0 || ^5.0",
        "ivory/google-map": "dev-sf5"
    },
    "require-dev": {
        "ivory/serializer-bundle": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-http/cache-plugin": "^1.4",
        "php-http/guzzle6-adapter": "^1.0 || ^2.0",
        "phpunit/phpunit": "^8.0",
        "phpunit/phpunit-selenium": "^8.0",
        "symfony/cache": "^3.4 || ^4.0 || ^5.0",
        "symfony/form": "^3.4 || ^4.0 || ^5.0",
        "symfony/phpunit-bridge": "^3.4 || ^4.0 || ^5.0",
        "symfony/templating": "^3.4 || ^4.0 || ^5.0",
        "symfony/yaml": "^3.4 || ^4.0 || ^5.0",
        "twig/twig": "^1.18 || ^2.0"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMapBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivory\\Tests\\GoogleMapBundle\\": "tests/",
            "Ivory\\Tests\\GoogleMap\\": "vendor/ivory/google-map/tests"
        }
    },
    "replace": {
        "egeloen/google-map-bundle": "^3.0.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ekyna/ivory-google-map"
        }
    ]
}
